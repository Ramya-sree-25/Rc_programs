#include <stdio.h>
int main() {
    int n, i, even_count = 0, odd_count = 0;
    printf("Enter the size of the array: ");
    scanf("%d", &n);
    int arr[n];
    printf("Enter the elements of the array: ");
    for (i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
        if (arr[i] % 2 == 0) {
            even_count++;
        } else {
            odd_count++;
        }
    }
    printf("Number of even elements: %d\n", even_count);
    printf("Number of odd elements: %d\n", odd_count);  
    return 0;
}
