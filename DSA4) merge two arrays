#include<stdio.h>
int main()
{
    int a[100],b[100],i,c[100],n1,n2;
    printf("enter n no.of elements:\n");
    scanf("%d",&n1);
    printf("enter n no.of elements:\n");
    scanf("%d",&n2);
    printf("enter a elements:\n");
    for(i=0;i<n1;i++)
    scanf("%d",&a[i]);
    printf("enter b elements:\n");
    for(i=0;i<n2;i++)
    scanf("%d",&b[i]);
    for(i=0;i<n1;i++)
    c[i]=a[i];
    for(i=0;i<n2;i++)
    c[n1+i]=b[i];
    for(i=0;i<n1+n2;i++)
    printf("%d",c[i]);
    return 0;
}
